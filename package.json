{
  "name": "cuc-api",
  "version": "0.0.1",
  "keywords": [
    "cuc", 
    "centro universitario de la costa", 
    "api"
  ],
  "scripts": {
    "seed": "node --env-file=.env ./src/migrations/index.js",
    "scrape": "node --env-file=.env  ./src/scrape/index.js ",
    "dev": "node --watch --env-file=.env ./src/index.js",
    "start": "node --env-file=.env ./src/index.js",
    "test": "vitest"
  },
  "description": "An open source unofficial API rest for the Centro Universitario de la Costa",
  "dependencies": {
    "express": "4.21.1",
    "morgan": "1.10.0",
    "pg": "8.13.1",
    "playwright": "1.48.2"
  },
  "devDependencies": {
    "dotenv": "16.4.5",
    "eslint": "8.57.1",
    "eslint-config-standard": "17.1.0",
    "globals": "15.11.0",
    "vitest": "2.1.3"
  },
  "author": "Ramón Hernández (Monki)",
  "license": "ISC"
}
